---
layout: none
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Not a Hat</title>
  <link href="http://www.notahat.com/"/>
  <link type="application/atom+xml" rel="self" href="http://www.notahat.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.notahat.com/</id>
  <author>
    <name>Pete Yandell</name>
    <email>pete@notahat.com</email>
  </author>

  {% for post in site.posts %}
    <entry>
      <id>http://notahat.com{{ post.id }}</id>
      <link type="text/html" rel="alternate" href="http://www.notahat.com{{ post.url }}"/>
      <title>{{ post.title | xml_escape }}</title>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <author>
        <name>Pete Yandell</name>
        <uri>http://notahat.com/</uri>
      </author>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}

</feed>
